{% extends 'admin/base.html.twig' %}

{% block title %}Audit
{% endblock %}

{% block body %}
	<div class="container">
		<h1>Audit</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ audit.id }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ audit.name }}</td>
				</tr>
				<tr>
					<th>Datecreation</th>
					<td>{{ audit.datecreation ? audit.datecreation|date('Y-m-d') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('audit_index') }}">back to list</a>

		<a href="{{ path('audit_edit', {'id': audit.id}) }}">edit</a>

		{{ include('audit/_delete_form.html.twig') }}
	</div>
{% endblock %}
